<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Event stages -->
        
        <record id="event_stage_create" model="event.stage">
            <field name="name">Mới</field>
            <field name="description">Freshly created</field>
            <field name="sequence">1</field>
        </record>
        <record id="event_stage_need_update" model="event.stage">
            <field name="name">Bổ sung</field>
            <field name="description">need_update</field>
            <field name="sequence">2</field>
        </record>
        <record id="event_stage_accept" model="event.stage">
            <field name="name">Đã duyệt</field>
            <field name="description">The event has been publicly announced</field>
            <field name="sequence">3</field>
        </record>
        <record id="event_stage_cancel" model="event.stage">
            <field name="name">Đã huỷ</field>
            <field name="description">Fully ended</field>
            <field name="sequence">4</field>
            <field name="pipe_end" eval="True"/>
            <field name="fold" eval="True"/>
        </record>
        <record id="event_stage_registration" model="event.stage">
            <field name="name">Mở đăng ký</field>
            <field name="description">The event has been cancelled</field>
            <field name="sequence">5</field>
            <field name="pipe_end" eval="True"/>
            <field name="fold" eval="True"/>
        </record>
        <record id="event_stage_inprogress" model="event.stage">
            <field name="name">Đang diễn ra</field>
            <field name="description">The event has been cancelled</field>
            <field name="sequence">6</field>
            <field name="pipe_end" eval="True"/>
            <field name="fold" eval="True"/>
        </record>
        <record id="event_stage_completed" model="event.stage">
            <field name="name">Đã kết thúc</field>
            <field name="description">The event has been cancelled</field>
            <field name="sequence">7</field>
            <field name="pipe_end" eval="True"/>
            <field name="fold" eval="True"/>
        </record>
     
    </data>
</odoo>
