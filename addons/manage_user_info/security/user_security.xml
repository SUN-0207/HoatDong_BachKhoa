<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <data noupdate="1">

    <record id="group_hcmut_user" model="res.groups">
      <field name="name">User</field>
      <field name="category_id" ref="base.module_category_hcmut"/>
      <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_hcmut_falcutian" model="res.groups">
      <field name="name">Falcutian</field>
      <field name="category_id" ref="base.module_category_hcmut"/>
      <field name="implied_ids" eval="[(4, ref('group_hcmut_user'))]"/>
    </record>

    <record id="group_hcmut_admin" model="res.groups">
      <field name="name">Admin</field>
      <field name="category_id" ref="base.module_category_hcmut"/>
      <field name="implied_ids" eval="[(4, ref('group_hcmut_falcutian'))]"/>
      <field name="users" eval="[(4, ref('base.user_admin'))]"/>
    </record>

  </data>

</odoo>